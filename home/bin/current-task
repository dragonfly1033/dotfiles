#!/bin/python

from datetime import datetime


def out(a):
    print(f'    {a}')
    quit()

def index(hr, m):
    hr = str(hr) if len(str(hr)) == 2 else '0'+str(hr)
    m = str(m) if len(str(m)) == 2 else '0'+str(m)
    return int(f'{hr}{m}')

time = datetime.now()
day, hour, minute = time.strftime('%w:%H:%M').split(':')
day = int(day)
time = index(hour, minute)

if time <= index(7, 0) or index(23, 0) <= time:
    out('Sleeping')
elif time <= index(7, 30):
    out('Getting Ready')
elif time <= index(8, 0):
    out('Relaxing')
elif index(12, 30) <= time <= index(13, 0):
    out('Lunch')
elif index(18, 0) <= time <= index(18, 30):
    out('Dinner')
elif index(18, 30) <= time <= index(20, 0):
    out('Supervision Work')

if day in [1, 3, 5]:
    if time <= index(9, 40):
        out('Working / Supervision Work')
    elif index(10, 0) <= time <= index(11, 0):
        out('Lecture')
    else:
        out('Working')    
elif day in [2, 4]:
    if time <= index(8, 40):
        out('Working / Supervision Work')
    elif index(9, 0) <= time <= index(10, 0):
        out('Lecture')
    elif index(11, 0) <= time <= index(12, 0):
        out('Lecture')
    elif index(13, 0) <= time <= index(14, 0):
        out('Supervision Work')
    elif index(14, 0) <= time <= index(16, 0):
        out('Lecture')
    else:
        out('Working') 
elif day == 6:
    if time <= index(8, 40):
        out('Working / Supervision Work')
    elif index(9, 0) <= time <= index(10, 0):
        out('Lecture')
    else:
        out('Working')
else:
    out('Working')

