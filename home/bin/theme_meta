if len(sys.argv) < len(list(themes.values())[0]):
	print('Not enough args. Provide values (0, 1, 2 for either) for all of: \n    ', end='')
	print(', '.join(list(themes.values())[0].keys()))
	quit()

safe = [True] if sys.argv[1] == '1' else [False] if sys.argv[1] == '0' else [True,False]
light = [True] if sys.argv[2] == '1' else [False] if sys.argv[2] == '0' else [True,False]

for i in themes:
	if themes[i]['safe'] in safe and themes[i]['light'] in light:
		print(i)
