print("Calculator")

def sind(a): return sin(degrees(a))
def cosd(a): return cos(degrees(a))
def tand(a): return tan(degrees(a))
def asind(a): return degrees(asin(a))
def acosd(a): return degrees(acos(a))
def atan2d(y, x): return degrees(atan2(a))

def sign(a):
	return 1 * (a > 0) -1 * (a < 0)

